{
  "name": "sample",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node ./src/main.ts",
    "start:debug": "nodemon",
    "tsc": "tsc",
    "build:prod": "npm run build",
    "start:prod": "node dist/src/main.js",
    "build": "tsc",
    "clean": "rimraf dist/",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:migration": "dotenv -e env.test --  typeorm migration:run",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test:migration && jest --config ./test/jest-e2e.json --runInBand ",
    "lint": "eslint '**/**/*.ts'",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "copy-files": "copyfiles -e test/fixtures/*.ts -u 1 test/fixtures/**  dist/test",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./ormconfig.ts",
    "migration:generate": "typeorm migration:create -n",
    "migration:run": "typeorm migration:run -t each",
    "migration:revert": "typeorm migration:revert",
    "db:sync": "npm run typeorm:cli schema:sync"
  },
  "keywords": [
    "typescript",
    "document-repository"
  ],
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/event-emitter": "0.0.2",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^1.0.0",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^7.0.0",
    "@types/jest": "27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/sinon": "^10.0.2",
    "azure-storage": "^2.10.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "date-fns": "2.23.0",
    "dotenv-cli": "^4.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "fast-csv": "^4.3.6",
    "hellosign-sdk": "1.6.1",
    "helmet": "^4.4.1",
    "into-stream": "^6.0.0",
    "jest": "27.0.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "moment": "^2.29.1",
    "nestjs-rollbar": "^1.4.1",
    "newrelic": "^7.1.0",
    "nock": "13.1.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "superagent-defaults": "^0.1.14",
    "swagger-ui-express": "^4.1.3",
    "ts-jest": "27.0.4",
    "typeorm": "^0.2.29",
    "url-join-ts": "^1.0.5",
    "uuidv4": "^6.2.6",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/multer": "^1.4.5",
    "@types/node": "^12.12.29",
    "@types/supertest": "^2.0.8",
    "@types/triple-beam": "^1.3.0",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "concurrently": "^5.0.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.27.0",
    "lodash": "^4.17.21",
    "mocha": "^6.2.2",
    "mock-jwks": "^0.3.1",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2"
  }
}
